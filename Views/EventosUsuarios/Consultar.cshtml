@model List<Parametros_Entrada>
@{
    ViewData["Title"] = "Consultar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://bootswatch.com/5/quartz/bootstrap.min.css">
</head>
<body>
    <div class="container">




        <div class="col-md-200">
            <table class="table table-bordered">

                <thead>
                    <tr>

                      
                        <th>Tipo de Producto</th>
                        <th>Nombre</th>
                        <th>Clave de Producto</th>
                        <th>Precio</th>
                        <th>Razón Social de Proveedor</th>
                        <th>Opciones</th>

                    </tr>
                </thead>

                <tbody>
                    @foreach(var x in Model)
                    {
                        <tr>

                            <td>@x.tipoProducto</td>
                            <td>@x.NombreProducto</td>
                            <td>@x.claveProducto</td>
                            <td>@x.precioProducto</td>
                            <td>@x.razonSocialProvedor</td>

                            <td>
                                <a asp-action="Editar" asp-controller="EventosUsuarios" asp-route-idProducto="@x.idProducto" class="badge bg-primary">Editar</a>
                                <a asp-action="Eliminar" asp-controller="EventosUsuarios" asp-route-idProducto="@x.idProducto" class="badge bg-primary">Eliminar</a><br>
                                <a asp-action="Agregar" asp-controller="EventosUsuarios" class="badge bg-primary">Crear Nuevo</a>

                            </td>
                            
                        </tr>
                        
                    }


                </tbody>

            </table>

        </div>

    </div>


</body>
</html>

